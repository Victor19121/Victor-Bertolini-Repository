/*
28. Faça a leitura de três valores e apresente como resultado a soma dos quadrados dos três
valores lidos.
*/

#include <stdio.h>

int main(){
    int num1, num2, num3, soma;
    printf("Digite três valores para ver a soma de seus quadrados: ");
    scanf("%d %d %d", &num1, &num2, &num3);
    soma = num1*num1 + num2*num2 + num3*num3;
    printf("O valor da soma do quadrado desses números é: %d", soma);
    return 0;
}
//=================================================================================================
/*
29. Leia quatro notas, calcule a média aritmética e imprima o resultado.

*/

#include <stdio.h>

int main(){
    float nota1, nota2, nota3, nota4, media;
    printf("Digite as 4 notas para ver a media: ");
    scanf("%f %f %f %f", &nota1, &nota2, &nota3, &nota4);
    media = (nota1 + nota2 + nota3 + nota4)/4;
    printf("O valor da media das notas é %.1f", media);
    return 0;
}
