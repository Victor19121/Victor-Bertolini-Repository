#include <stdio.h>

int main(){
    float M, H;
    printf("Digite um valor em hectares para vê-lo em m2: ");
    scanf("%f", &H);
    M = H*10000;
    printf("O valor %.2f hectares é igual a %.2f m2", H, M);
    return 0;
}
