int main(){
    float m3, litros;
    printf("Digite um valor em metros cúbicos para vê-lo em litros: ");
    scanf("%f", &m3);
    litros = m3*1000;
    printf("O valor %.2f metros cúbicos é igual a %.2f Litros", m3, litros);
    return 0;
}
